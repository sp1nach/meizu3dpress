<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorAccent">#FF4081</color>
    <color name="colorPrimary">#3F51B5</color>
    <color name="colorPrimaryDark">#303F9F</color>
    <color name="launcher_list_item_pressed">#19000000</color>
    <declare-styleable name="PeekAndPopTheme"><attr format="reference" name="SlideSelectListViewStyle"/></declare-styleable>
    <dimen name="heavily_press_distance">30dp</dimen>
    <dimen name="launcher_distance_between_icon_and_menu">15dp</dimen>
    <dimen name="launcher_horizontal_offset">5dp</dimen>
    <dimen name="menu_min_height">58dp</dimen>
    <dimen name="preview_height">497dp</dimen>
    <dimen name="preview_width">342dp</dimen>
    <dimen name="pulldownview_state_changed_distance">100dp</dimen>
    <dimen name="start_show_menu_distance">58dp</dimen>
    <dimen name="status_bar_height">25dip</dimen>
    <string name="app_name">ForceTouch</string>
    <string name="has_mark_read">Has mark read</string>
    <string name="has_mark_unread">Has mark unread</string>
    <string name="pull_to_mark_read">Pull to mark read</string>
    <string name="pull_to_mark_unread">Pull to mark unread</string>
    <style name="SlideSelectListViewStyle" parent="@android:style/Widget.ListView.DropDown">
    </style>
    <style name="SlideSelectListViewStyle.App">
        <item name="android:listSelector">@drawable/app_list_item_pressed</item>
        <item name="android:divider">@drawable/app_list_item_divider</item>
        <item name="android:background">@drawable/peek_pop_menu_bg</item>
    </style>
    <style name="SlideSelectListViewStyle.Launcher">
        <item name="android:listSelector">@drawable/launcher_list_selector_background</item>
        <item name="android:divider">@drawable/app_list_item_divider</item>
    </style>
    <style name="Transparent">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
        <!--<item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item>-->
    </style>
</resources>